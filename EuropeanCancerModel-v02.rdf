    <!-- http://.www.hl7.eu/oncology/EuropeanCancerModel#DiseaseStatusType -->

    <rdf:Description rdf:about="http://.www.hl7.eu/oncology/EuropeanCancerModel#DiseaseStatusType">
        <rdfs:comment>It could be one of the following: 
    Progression (in case the cancer is grown respect the previous Visit)
    Partial Remission (in case the cancer shrunk respect the previous Visit)
    Complete Remission (in case the cancer completely disappeared)
    Recurrence (in case the cancer after complete remission re-appeared or cancer that has recurred (come back), usually after a period of time during which the cancer could not be detected). </rdfs:comment>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Complete Remission</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Partial Remission</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>Progression</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>Recurrence</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                        <rdf:first>Stable</rdf:first>
                                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdf:Description>
    
    <!-- http://.www.hl7.eu/oncology/EuropeanCancerModel#IntentType -->


    <rdf:Description rdf:about="http://.www.hl7.eu/oncology/EuropeanCancerModel#IntentType">
        <rdfs:comment>Type of Intent (Palliative or Curative (Definitive) )</rdfs:comment>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
