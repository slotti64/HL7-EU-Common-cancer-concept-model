        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#Evidence"/>
                    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#TreatmentResponse"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#Evidence"/>
                    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#Procedure"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>based on</rdfs:label>
    </owl:ObjectProperty>
    


