    <!-- http://HL7.eu/ontology/CommonCancerModel#BaseOfAssertion -->


    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#BaseOfAssertion">
        <rdfs:comment>the type of procedure on the base of the Cancer Condition was detected. It could be a choice from Visit, Biopsy, Surgery, Imaging, Biomarker, Lab Report, ... </rdfs:comment>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Biomarker</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Biopsy</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>Genomics</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>Imaging</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                        <rdf:first>Lab Report</rdf:first>
                                                        <rdf:rest>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                                <rdf:first>Visit</rdf:first>
                                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdf:Description>
    


    <!-- http://HL7.eu/ontology/CommonCancerModel#DiseaseStatus -->


    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#DiseaseStatus">
        <rdfs:comment>&lt;html&gt;
  &lt;head&gt;
		&lt;style&gt;
			p {padding:0px; margin:0px;}
		&lt;/style&gt;
	&lt;/head&gt;
  &lt;body&gt;
&lt;span style=&quot;color:black;&quot;&gt;Represents the status of the&amp;#160;&lt;em&gt;Cancer Condition &lt;/em&gt;evaluated during a &lt;em&gt;Visit&lt;/em&gt;. It could be one of the following:&amp;#160;&lt;/span&gt;
    &lt;ul&gt;
      &lt;li style=&quot;color:black;&quot;&gt;
&lt;span style=&quot;color:black;&quot;&gt;&lt;em&gt;Progression: &lt;/em&gt;in case the cancer is grown respect the previous &lt;em&gt;Visit&lt;/em&gt;&lt;/span&gt;

      &lt;/li&gt;
      &lt;li style=&quot;color:black;&quot;&gt;
&lt;span style=&quot;color:black;&quot;&gt;&lt;em&gt;Stable Disease:&amp;#160;&lt;/em&gt;in case the cancer is not grown or&amp;#160;shrunk respect the previous &lt;em&gt;Visit&lt;/em&gt;&lt;/span&gt;

      &lt;/li&gt;
      &lt;li style=&quot;color:black;&quot;&gt;
&lt;span style=&quot;color:black;&quot;&gt;&lt;em&gt;Partial Remission:&amp;#160;&lt;/em&gt;in case the cancer shrunk respect the previous &lt;em&gt;Visit&lt;/em&gt;&lt;/span&gt;

      &lt;/li&gt;
      &lt;li style=&quot;color:black;&quot;&gt;
&lt;span style=&quot;color:black;&quot;&gt;&lt;em&gt;Complete Remission: &lt;/em&gt;in case the cancer completely disappeared&lt;/span&gt;

      &lt;/li&gt;
      &lt;li style=&quot;color:black;&quot;&gt;
&lt;span style=&quot;color:black;&quot;&gt;&lt;em&gt;Recurrence: &lt;/em&gt;in case the cancer after complete remission re-appeared or cancer that has recurred (come back), usually after a period of time during which the cancer &lt;strong&gt;could not be&amp;#160;detected&lt;/strong&gt;).&amp;#160;&lt;/span&gt;

      &lt;/li&gt;
    &lt;/ul&gt;
&lt;/body&gt;
&lt;/html&gt;</rdfs:comment>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Complete Remission</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Disease Recurrence</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>Partial Remission</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>Progression</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                        <rdf:first>Stable</rdf:first>
                                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdf:Description>
    


    <!-- http://HL7.eu/ontology/CommonCancerModel#EvolutionType -->


    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#EvolutionType">
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Progression</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Recurrence</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdf:Description>
    


    <!-- http://HL7.eu/ontology/CommonCancerModel#ExtentType -->


    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#ExtentType">
        <rdfs:comment>&lt;html&gt;
  &lt;head&gt;
		&lt;style&gt;
			p {padding:0px; margin:0px;}
		&lt;/style&gt;
	&lt;/head&gt;
  &lt;body&gt;
&lt;span style=&quot;color:black;&quot;&gt;It can be:&lt;/span&gt;
    &lt;ul&gt;
      &lt;li&gt;
&lt;span style=&quot;color:black;&quot;&gt;&lt;em&gt;Local&lt;/em&gt;: in case the&amp;#160;site is the primary site (the site of the&amp;#160;&lt;em&gt;Cancer Condition&lt;/em&gt; is the same)&lt;/span&gt;

      &lt;/li&gt;
      &lt;li&gt;
&lt;span style=&quot;color:black;&quot;&gt;&lt;em&gt;Loco-regional&lt;/em&gt;: i&lt;span&gt;n case the site is the primary site and the surrounding anatomical region (also to the regional lymph nodes but with no distant metastases)&lt;/span&gt;&lt;/span&gt;

      &lt;/li&gt;
      &lt;li&gt;
&lt;span style=&quot;color:black;&quot;&gt;&lt;em&gt;Metastatic&lt;/em&gt;: in case there are also distant metastases.&lt;/span&gt;

      &lt;/li&gt;
    &lt;/ul&gt;
&lt;/body&gt;
&lt;/html&gt;</rdfs:comment>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Local</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Loco-regional</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>Metastatic</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdf:Description>
    


    <!-- http://HL7.eu/ontology/CommonCancerModel#Intent -->


    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#Intent">
        <rdfs:comment>An Intent: indicates if it is curative or not (so only to decrease the symptoms) for the cancer. It can be a choice from: Definitive and Palliative.</rdfs:comment>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Definitive (curative)</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Palliative</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdf:Description>
    


    <!-- http://HL7.eu/ontology/CommonCancerModel#LifeStatus -->


    <rdf:Description rdf:about="http://HL7.eu/ontology/CommonCancerModel#LifeStatus">
        <rdfs:comment>&lt;html&gt;
  &lt;head&gt;
		&lt;style&gt;
			p {padding:0px; margin:0px;}
		&lt;/style&gt;
	&lt;/head&gt;
  &lt;body&gt;
    &lt;p&gt;
&lt;strong&gt;Life Status (and Cause)&lt;/strong&gt;&lt;em&gt;&amp;#160;&lt;/em&gt;that represent the evolving life status of the patient verified during a &lt;em&gt;Visit, &lt;/em&gt;&lt;u&gt;after the first treatment&lt;/u&gt;, to indicate if it is alive (with disease or not) or dead (with information on it). &lt;span&gt;It can evolve during the time. It could be one of the following:&amp;#160;&lt;/span&gt;

    &lt;/p&gt;

    &lt;ul&gt;
      &lt;li data-uuid=&quot;8065c29c-71d9-4d21-a159-a24e6cbf6e1b&quot;&gt;
&lt;span&gt;&lt;em&gt;Alive, No Evidence of Disease (NED)&lt;/em&gt;: in case it is alive and there are no evidence of the disease presence anymore&lt;/span&gt;

      &lt;/li&gt;
      &lt;li data-uuid=&quot;c3b87f80-fffd-44bd-b52f-39a64429950f&quot;&gt;
&lt;span&gt;&lt;em&gt;Dead of Disease (DOD)&lt;/em&gt;: in case it is dead with cancer as sure reason&lt;/span&gt;

      &lt;/li&gt;
      &lt;li data-uuid=&quot;b83789d3-805c-41d5-af77-180e4df33856&quot;&gt;
&lt;span&gt;&lt;em&gt;Dead of Other Cause (DOC)&lt;/em&gt;: in case it is dead, but the reason is not cancer for sure&lt;/span&gt;

      &lt;/li&gt;
      &lt;li data-uuid=&quot;4819a46a-b4b5-4ad7-b9bc-d8eed79d7ac6&quot;&gt;
&lt;em&gt;Dead of Unknow Cause (DUC)&lt;/em&gt;: &lt;span&gt;in case it is dead, but the reason is unknow (it could be or not cancer)&lt;/span&gt;

      &lt;/li&gt;
      &lt;li data-uuid=&quot;a525ee59-dc19-4a87-8876-ef326ac71576&quot;&gt;
&lt;span&gt;&lt;em&gt;Alive With Disease (AWD)&lt;/em&gt;: in case it is alive and the cancer it is still present.&lt;/span&gt;

      &lt;/li&gt;
    &lt;/ul&gt;
&lt;/body&gt;
&lt;/html&gt;</rdfs:comment>
        <owl:equivalentClass>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>Alive No Evidence of Disease (NED)</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>Alive with Disease (AWD)</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>Dead of Disease (DOD)</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>Dead of Other Cause (DOC)</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                        <rdf:first>Dead of Unknown Cause (DUC)</rdf:first>
                                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:equivalentClass>
    </rdf:Description>
    


