<http://HL7.eu/ontology/CommonCancerModel> rdf:type owl:Ontology ;
                                            owl:versionIRI <urn:absolute:2.1> ;
                                            rdfs:label "HL7 Common Cancer Model" .

#################################################################
#    Datatypes
#################################################################

###  http://HL7.eu/ontology/CommonCancerModel#BaseOfAssertion
:BaseOfAssertion rdfs:comment "the type of procedure on the base of the Cancer Condition was detected. It could be a choice from Visit, Biopsy, Surgery, Imaging, Biomarker, Lab Report, ... " ;
                 owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                       owl:oneOf [ rdf:type rdf:List ;
                                                   rdf:first "Biomarker" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "Biopsy" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "Genomics" ;
                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                    rdf:first "Imaging" ;
                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                               rdf:first "Lab Report" ;
                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                          rdf:first "Visit" ;
                                                                                                          rdf:rest rdf:nil
                                                                                                        ]
                                                                                             ]
                                                                                  ]
                                                                       ]
                                                            ]
                                                 ]
                                     ] .


###  http://HL7.eu/ontology/CommonCancerModel#DiseaseStatus
:DiseaseStatus rdfs:comment """<html>
  <head>
		<style>
			p {padding:0px; margin:0px;}
		</style>
	</head>
  <body>
<span style=\"color:black;\">Represents the status of the&#160;<em>Cancer Condition </em>evaluated during a <em>Visit</em>. It could be one of the following:&#160;</span>
    <ul>
      <li style=\"color:black;\">
<span style=\"color:black;\"><em>Progression: </em>in case the cancer is grown respect the previous <em>Visit</em></span>

      </li>
      <li style=\"color:black;\">
<span style=\"color:black;\"><em>Stable Disease:&#160;</em>in case the cancer is not grown or&#160;shrunk respect the previous <em>Visit</em></span>

      </li>
      <li style=\"color:black;\">
<span style=\"color:black;\"><em>Partial Remission:&#160;</em>in case the cancer shrunk respect the previous <em>Visit</em></span>

      </li>
      <li style=\"color:black;\">
<span style=\"color:black;\"><em>Complete Remission: </em>in case the cancer completely disappeared</span>

      </li>
      <li style=\"color:black;\">
<span style=\"color:black;\"><em>Recurrence: </em>in case the cancer after complete remission re-appeared or cancer that has recurred (come back), usually after a period of time during which the cancer <strong>could not be&#160;detected</strong>).&#160;</span>

      </li>
    </ul>
</body>
</html>""" ;
               owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                     owl:oneOf [ rdf:type rdf:List ;
                                                 rdf:first "Complete Remission" ;
                                                 rdf:rest [ rdf:type rdf:List ;
                                                            rdf:first "Disease Recurrence" ;
                                                            rdf:rest [ rdf:type rdf:List ;
                                                                       rdf:first "Partial Remission" ;
                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                  rdf:first "Progression" ;
                                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                                             rdf:first "Stable" ;
                                                                                             rdf:rest rdf:nil
                                                                                           ]
                                                                                ]
                                                                     ]
                                                          ]
                                               ]
                                   ] .


###  http://HL7.eu/ontology/CommonCancerModel#EvolutionType
:EvolutionType owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                     owl:oneOf [ rdf:type rdf:List ;
                                                 rdf:first "Progression" ;
                                                 rdf:rest [ rdf:type rdf:List ;
                                                            rdf:first "Recurrence" ;
                                                            rdf:rest rdf:nil
                                                          ]
                                               ]
                                   ] .


###  http://HL7.eu/ontology/CommonCancerModel#ExtentType
:ExtentType rdfs:comment """<html>
  <head>
		<style>
			p {padding:0px; margin:0px;}
		</style>
	</head>
  <body>
<span style=\"color:black;\">It can be:</span>
    <ul>
      <li>
<span style=\"color:black;\"><em>Local</em>: in case the&#160;site is the primary site (the site of the&#160;<em>Cancer Condition</em> is the same)</span>

      </li>
      <li>
<span style=\"color:black;\"><em>Loco-regional</em>: i<span>n case the site is the primary site and the surrounding anatomical region (also to the regional lymph nodes but with no distant metastases)</span></span>

      </li>
      <li>
<span style=\"color:black;\"><em>Metastatic</em>: in case there are also distant metastases.</span>

      </li>
    </ul>
</body>
</html>""" ;
            owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "Local" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "Loco-regional" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "Metastatic" ;
                                                                    rdf:rest rdf:nil
                                                                  ]
                                                       ]
                                            ]
                                ] .


###  http://HL7.eu/ontology/CommonCancerModel#Intent
:Intent rdfs:comment "An Intent: indicates if it is curative or not (so only to decrease the symptoms) for the cancer. It can be a choice from: Definitive and Palliative." ;
        owl:equivalentClass [ rdf:type rdfs:Datatype ;
                              owl:oneOf [ rdf:type rdf:List ;
                                          rdf:first "Definitive (curative)" ;
                                          rdf:rest [ rdf:type rdf:List ;
                                                     rdf:first "Palliative" ;
                                                     rdf:rest rdf:nil
                                                   ]
                                        ]
                            ] .


###  http://HL7.eu/ontology/CommonCancerModel#LifeStatus
:LifeStatus rdfs:comment """<html>
  <head>
		<style>
			p {padding:0px; margin:0px;}
		</style>
	</head>
  <body>
    <p>
<strong>Life Status (and Cause)</strong><em>&#160;</em>that represent the evolving life status of the patient verified during a <em>Visit, </em><u>after the first treatment</u>, to indicate if it is alive (with disease or not) or dead (with information on it). <span>It can evolve during the time. It could be one of the following:&#160;</span>

    </p>

    <ul>
      <li data-uuid=\"8065c29c-71d9-4d21-a159-a24e6cbf6e1b\">
<span><em>Alive, No Evidence of Disease (NED)</em>: in case it is alive and there are no evidence of the disease presence anymore</span>

      </li>
      <li data-uuid=\"c3b87f80-fffd-44bd-b52f-39a64429950f\">
<span><em>Dead of Disease (DOD)</em>: in case it is dead with cancer as sure reason</span>

      </li>
      <li data-uuid=\"b83789d3-805c-41d5-af77-180e4df33856\">
<span><em>Dead of Other Cause (DOC)</em>: in case it is dead, but the reason is not cancer for sure</span>

      </li>
      <li data-uuid=\"4819a46a-b4b5-4ad7-b9bc-d8eed79d7ac6\">
<em>Dead of Unknow Cause (DUC)</em>: <span>in case it is dead, but the reason is unknow (it could be or not cancer)</span>

      </li>
      <li data-uuid=\"a525ee59-dc19-4a87-8876-ef326ac71576\">
<span><em>Alive With Disease (AWD)</em>: in case it is alive and the cancer it is still present.</span>

      </li>
    </ul>
</body>
</html>""" ;
            owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "Alive No Evidence of Disease (NED)" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "Alive with Disease (AWD)" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "Dead of Disease (DOD)" ;
                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                               rdf:first "Dead of Other Cause (DOC)" ;
                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                          rdf:first "Dead of Unknown Cause (DUC)" ;
                                                                                          rdf:rest rdf:nil
                                                                                        ]
                                                                             ]
                                                                  ]
                                                       ]
                                            ]
                                ] .


