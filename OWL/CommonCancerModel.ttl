                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :dependingOnProcedure ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] ;
               rdfs:comment """<html>
  <head>
		<style>
			p {padding:0px; margin:0px;}
		</style>
	</head>
  <body>
<span style=\"color:black;\">Represents the type of biomarker on the base of the&#160;<em>Cancer Condition</em></span><span style=\"color:black;\"><span> was detected.&#160;</span></span></body>
</html>""" ;
               rdfs:label "Biomarker Type" .


###  http://HL7.eu/ontology/CommonCancerModel#Biopsy
:Biopsy rdf:type owl:Class ;
